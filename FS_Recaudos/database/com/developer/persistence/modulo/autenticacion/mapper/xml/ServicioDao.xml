<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.developer.persistence.modulo.autenticacion.mapper.dao.ServicioDao">
	
	<select id="getURLSPorServicio" parameterType="servicio" resultType="java.lang.String">
		select surl_url from aut_tsurl
		where surl_servicio = #{servicio_servicio}
	</select>
	
	
	

</mapper>